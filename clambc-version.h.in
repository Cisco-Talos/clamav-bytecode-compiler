#ifndef CLAMBC_VER_H
#define CLAMBC_VER_H

/**
 * @macro
 * Version number of the clambcc package release
 */
#define CLAMBC_VERSION "@PACKAGE_VERSION@"

/**
 * @macro
 * Numerical representation of the version number of the clambcc package
 * release. This is a 24 bit number with 8 bits for major number, 8 bits
 * for minor and 8 bits for patch. Version 1.2.3 becomes 0x010203.
 */
#define CLAMBC_VERSION_NUM @PACKAGE_VERSION_NUM@

/**
 * @macro
 * Version number of the clambcc library release
 */
#define LIBCLAMBC_VERSION "@LIBCLAMBC_VERSION@"

/**
 * @macro
 * Numerical representation of the version number of the libclambcc library
 * release. This is a 24 bit number with 8 bits for major number, 8 bits
 * for minor and 8 bits for patch. Version 1.2.3 becomes 0x010203.
 */
#define LIBCLAMBC_VERSION_NUM @LIBCLAMBC_VERSION_NUM@

#endif /* CLAMBC_VER_H */
