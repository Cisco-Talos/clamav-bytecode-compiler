#ifndef PREVISIT
#define PREVISIT(NODE)
#endif

#ifndef POSTVISIT
#define POSTVISIT(NODE)
#endif

PREVISIT(ArraySubscriptExpr)
PREVISIT(BinaryOperator)
PREVISIT(CallExpr)
PREVISIT(CastExpr)
PREVISIT(DeclStmt)
PREVISIT(ObjCMessageExpr)
PREVISIT(ReturnStmt)

POSTVISIT(CallExpr)
POSTVISIT(BinaryOperator)

#undef PREVISIT
#undef POSTVISIT
